{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server;Database=your_db;User Id=your_user;Password=your_password;",
    "AuthConnection": "Server=your_server;Database=your_db;User Id=your_user;Password=your_password;"
  },
  "JwtSettings": {
    "Secret": "your_jwt_secret_key_here",
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "ExpiryMinutes": 60
  }
}
